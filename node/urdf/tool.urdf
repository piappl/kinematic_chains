<robot name="test_robot">
  <link name="link1" />
  <link name="link2" />
  <link name="link3" />
  <link name="link4" />
  <link name="link5" />


  <joint name="joint0" type="fixed">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0 0 0.05" rpy="0 0 0" />
  </joint>

  <joint name="joint1" type="continuous">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0 0 0.05" rpy="0 0 0" />
    <axis xyz="0 0 1"/>
  </joint>

  <joint name="joint2" type="continuous">
    <parent link="link3"/>
    <child link="link5"/>
    <origin xyz="0 0 0.0" rpy="0 0 0" />
    <axis xyz="1 0 0"/>
  </joint>


  <joint name="joint3" type="continuous">
    <parent link="link5"/>
    <child link="link4"/>
    <origin xyz="0 0 0.05" rpy="0 0 0" />
    <axis xyz="0 0 1 "/>
  </joint>
</robot>
